@model KeyValuePair<string, OnlineDiary.Models.Diary.ScheduleLesson[]>
<div class="col-md-4 col-sm-6 col-xs-12">
    <table class="table table-bordered b-preview_table">
        <thead>
            <tr>
                <th colspan="2">@Model.Key</th>
                <th colspan="2">@ViewData["day"]</th>
            </tr>
            <tr>
                <th width="10%"># п/п</th>
                <th width="40%">Время</th>
                <th width="40%" class="b-preview_subject">Предмет</th>                
                <th width="10%">Дз</th>
            </tr>
        </thead>
        <tbody>
            @for (int i = 0; i < OnlineDiary.Models.Diary.ScheduleLesson.MAX_LESSONS_PER_DAY; i++) {
                <tr>
                    <td>@(i + 1)</td>
                    <td>8:00 - 8:30</td>
                    @if (Model.Value.Length > i && Model.Value[i] != null) {
                        <td class="b-preview_subject" data-lesson-id="@Model.Value[i].Id.ToString()">@Model.Value[i].Lesson.Title</td>
                    } else {
                        <td class="b-preview_subject">Нет урока</td>
                    }
                    <td>
                        <span class="glyphicon glyphicon-eye-open"></span>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>